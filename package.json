{
  "name": "restbus",
  "description": "RESTful JSON API for the NextBus Inc. public XML feed.",
  "version": "2.4.0",
  "main": "index",
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.18.2",
    "morgan": "1.9.1",
    "xml2js": "0.5.0"
  },
  "devDependencies": {
    "@codegenie/serverless-express": "^4.13.0",
    "prettier": "^3.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morganney/restbus.git"
  },
  "homepage": "http://restbus.info",
  "keywords": [
    "umo",
    "nextbus",
    "api",
    "rest",
    "json",
    "proxy"
  ],
  "bugs": {
    "url": "https://github.com/morganney/restbus/issues"
  },
  "author": {
    "name": "Morgan Ney <morganney@gmail.com>",
    "url": "https://morgan.neys.info"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier -w .",
    "start": "node index.js run",
    "dev": "node --watch index.js run",
    "build": "NODE_ENV=production webpack",
    "zip": "cd dist && zip restbus.zip index.js"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "printWidth": 90,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
